{
    "require": {
        "asgrim/mini-mezzio": "^2.2",
        "laminas/laminas-db": "^2.20",
        "laminas/laminas-hydrator": "^4.16",
        "laminas/laminas-servicemanager": "^4.4",
        "mezzio/mezzio-fastroute": "^3.13",
        "symfony/console": "^7.3",
        "twilio/sdk": "^8.7",
        "vlucas/phpdotenv": "^5.6",
        "akselmj/weatherapi-php-client": "dev-add-hydration-support"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S 0.0.0.0:8080 -t public/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/settermjd/weatherapi-php-client.git"
        }
    ]
}
